<%- include("header_main") -%> 
    <title>Botai | Logout</title>
    <script type="text/javascript" src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v9.0&appId=189993795563654&autoLogAppEvents=1" nonce="pG1IzRJN"></script>
</head>
<body>
  <script>
    FB.getLoginStatus(function(response) {
      if (response && response.status === 'connected') {
        FB.logout(function(response) {
          $('#loader').hide(); 
          document.getElementById("msg1").innerHTML = "You are now logged out!"
          document.getElementById("msg2").innerHTML = "See you again shortly!"
        });
      } else {
        window.location.reload(); 
      }
    });  
  </script>
  <div class="form-group col-md-12" style="margin-top:180px;margin-bottom:50px;">
    <div class="d-flex justify-content-center">
      <h2 id = "msg1"></h2>
    </div>
    <div class="d-flex justify-content-center" >
      <div class="spinner-grow text-primary" style="width: 6rem; height: 6rem; margin-top:160px; margin-bottom:150px" role="status" id="loader">
      </div>
    <div class="d-flex justify-content-center">
      <h4 id = "msg2" style="margin-top:25px;margin-bottom:80px;"></h4>
    </div>
  </div>
</body>
<%- include("footer") -%>